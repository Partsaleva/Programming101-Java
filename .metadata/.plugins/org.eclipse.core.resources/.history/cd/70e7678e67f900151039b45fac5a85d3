package matrices;

import java.util.Random;

public class Multiplier implements Runnable{

	private int row;
	private int col;
	private int ch;
	public Multiplier(int row,int col){
		this.row=row;
		this.col=col;
		this.ch=0;
	}
	public synchronized void multiplyMatrix(Matrix A, Matrix B){

	    int sum=0;
	    for(int a=0;a<row;a++){
	        sum=0;
	        for(int b=0;b<col;b++){
	            sum=sum+mat[chance][b]*mat2[b][a];
	        }
	        result[ch][a]=sum;
	    }

	    if(ch>=row)
	        return;
	    ch++;
	}
	@Override
	public void run() {
		// TODO Auto-generated method stub
		
	}
	
	

	
	
	

}
