package week16;

public class Member {

	private int coef;
	private int var;
	private int degree;
	
	public Member(int coef, int var, int degree) {
		this.coef = coef;
		this.var = var;
		this.degree = degree;
	}

	public int getCoef() {
		return coef;
	}

	public int getVar() {
		return var;
	}

	public int getDegree() {
		return degree;
	}

	@Override
	public String toString() {
		return "" + coef + "" + var + "^" + degree + "";
	}
	
	
}
