import java.io.FileWriter;
import java.io.IOException;

public class Test {

	public static void main(String[] args) {
		try(BufferedWriter out = new BufferedWriter(new FileWriter("test.csv"))){
			String line="data"+ "dat"+"\n"+"dat";
			out.write(line);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

	}

}
