package week16;

public class Succ extends NaturalNumber{

	public static void main(String[] args) {
		Succ s=new Succ(new Zero());
		System.out.println(s.toString());

	}

	public static int count=0;
	String succ;
	private NaturalNumber n;
	
	public  Succ(NaturalNumber n) {
		this.n=n;
		succ="Succ + "+ n;
		count++;
	}
	
	
	@Override
	boolean isZero() {
		return false;
	}

	@Override
	NaturalNumber next() {
		return new Succ(this);
	}

	@Override
	NaturalNumber prev() {
		return n;
	}

	@Override
	NaturalNumber add(NaturalNumber a) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	NaturalNumber minus(NaturalNumber a) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	Integer toInt() {
		return count;
	}

	@Override
	public String toString() {
		return "Succ + "+ prev().toString() ;
	}


	


	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Succ other = (Succ) obj;
		if (succ == null) {
			if (other.succ != null)
				return false;
		} else if (!succ.equals(other.succ))
			return false;
		return true;
	}


	

}
