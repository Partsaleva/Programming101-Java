package week16;

public class Succ extends NaturalNumber{

	public static void main(String[] args) {
		Succ s=new Succ(new Zero());
		System.out.println(s.toString());

	}

	public static int count=0;
	String succ;
	
	public  Succ(NaturalNumber n) {
		succ="Succ";
		count++;
		addString();
	}
	
	String addString(){
		StringBuilder sb=new StringBuilder();
		sb.append("Succ + ");
		return sb.toString();		
	}
	@Override
	boolean isZero() {
		return false;
	}

	@Override
	NaturalNumber next() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	NaturalNumber prev() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	NaturalNumber add(NaturalNumber a) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	NaturalNumber minus(NaturalNumber a) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	Integer toInt() {
		return count;
	}

	@Override
	public String toString() {
		return addString();
	}

	@Override
	public boolean equals() {
		// TODO Auto-generated method stub
		return false;
	}

}
