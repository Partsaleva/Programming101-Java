package educationalSystem.models;

public class Answer {
	private int ansId;
	private int qustId;
	private String data;
	private boolean isCorrect;
	
	public Answer(int ansId, int qustId, String data, boolean isCorrect) {
		this.ansId = ansId;
		this.qustId = qustId;
		this.data = data;
		this.isCorrect = isCorrect;
	}
	
	
}
