	public static void main(String[] args) {
		WebCrawler spider = new WebCrawler();
        try {
			spider.search("http://9gag.com/", "programmer");
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

	}

}
