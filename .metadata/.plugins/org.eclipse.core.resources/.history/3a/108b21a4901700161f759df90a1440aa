package week16;

import java.util.ArrayList;
import java.util.List;

public class Run {

	public static void main(String[] args) {
		String p1="2x^4 + 3x^2 - 10x + 3";
		String p2="8x^3 + 6x - 10";
		parse(p1);

	}

	public static void parse(String p){
		List<Member> members=new ArrayList<Member>();
		String[] parts=p.split(" ");
		for (String mem : parts) {
			String m=mem.replace("x", "");
			//System.out.println(m);
			String[] memberParts=m.split("^");
			if(memberParts.length>1){
			System.out.println(memberParts[0]);
			System.out.println(memberParts[1]);
			}else System.out.println(memberParts[0]);
				
		}
	}
}
