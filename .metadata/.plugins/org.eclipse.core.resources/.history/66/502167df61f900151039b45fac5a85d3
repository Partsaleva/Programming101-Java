package matrices;

import java.util.Random;

public class Multiplier {

	//fill matrix with random numbers
	public void fillMatrix(Matrix m){
		Random r=new Random();
		for (int i = 0; i < m.getRow(); i++) {
			for (int j = 0; j <m.getCol(); j++) {
				m.getMatrix()[i][j]=r.nextInt(100);
			}
		}		
	}
	

}
