package managers;

import java.util.List;

import models.Drone;
import models.Warehouse;

public class DronesControl {

	public List<Drone> chooseDrones(Warehouse warehouse, int deliveryWeight,
			int deliveryTime) {
		int weight=deliveryWeight;
		while(weight>0){
			Drone drone=drones.poll();
			dronesForDelivery.add(drone);
			weight -= drone.getWeightCapacity();
		}
	}

}
